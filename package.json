{
  "name": "video-component",
  "version": "0.0.0",
  "description": "Custom HTML5 video player that responds to events",
  "scripts": {
    "webpack": "webpack --watch --progress --profile --colors --display-error-details --display-cached",
    "single-page-server": "single-page-server -b=app/client -f=index.html",
    "http-server": "http-server",
    "start": "concurrently \"npm run webpack\" \"npm run single-page-server\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/prendus/video-component.git"
  },
  "homepage": "https://bitbucket.org/prendus/video-component#readme",
  "dependencies": {
    "firebase": "^3.0.3",
    "prendus-services": "git+ssh://git@bitbucket.org/prendus/prendus-services.git",
    "video.js": "^5.9.2"
  },
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-3": "^6.5.0",
    "concurrently": "^2.0.0",
    "http-server": "^0.9.0",
    "single-page-server": "0.1.0",
    "ts-loader": "0.8.1",
    "typescript": "1.8.7",
    "webpack": "1.12.14"
  }
}
